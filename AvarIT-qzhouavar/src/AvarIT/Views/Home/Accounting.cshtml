@{
    ViewData["Title"] = "Home Page";
}
<h2>Collapsible Panel</h2>
<div class="panel-group">
    <div class="panel panel-default">
        <div class="panel-heading">
            <h4 class="panel-title">
                @*<a data-toggle="collapse" href="#collapse1">*@Collapsible panel@*</a>*@
            </h4>
        </div>
        <div id="collapse1" @*class="panel-collapse collapse"*@>
            <div class="panel-body">
                Panel Body
                <div id="myCarousel" class="carousel slide" data-ride="carousel" data-interval="6000">
                    <ol class="carousel-indicators">
                        <li data-target="#myCarousel" data-slide-to="0" class="active"></li>
                        <li data-target="#myCarousel" data-slide-to="1"></li>
                        <li data-target="#myCarousel" data-slide-to="2"></li>
                        <li data-target="#myCarousel" data-slide-to="3"></li>
                    </ol>
                    <div class="carousel-inner" role="listbox">
                        <div class="item active">
                            <canvas id="canvas"></canvas>
                        </div>
                    </div>
                </div>
            </div>
            <div class="panel-footer">
                Panel Footer
             
                <!-- /.row -->
                <div class="row">
                    <div class="col-lg-3 col-md-6">
                        <div class="panel panel-primary">
                            <div class="panel-heading">
                                <div class="row">
                                    <div class="col-xs-3">
                                        <img src="~/images/computer.png" width="80" />
                                    </div>
                                    <div class="col-xs-9 text-right">
                                        <div class="huge">@ViewData["ComputerCaseCount"]</div>
                                        <div><a asp-controller="ComputerCases" asp-action="Create" style="color:white">New Computer Case!</a></div>

                                    </div>
                                </div>
                            </div>
                            <a asp-controller="ComputerCases" asp-action="Index">
                                <div class="panel-footer">
                                    <span class="pull-left">View Details</span>
                                    <span class="pull-right"><i class="fa fa-arrow-circle-right"></i></span>
                                    <div class="clearfix"></div>
                                </div>
                            </a>
                        </div>
                    </div>
                    <div class="col-lg-3 col-md-6">
                        <div class="panel panel-green">
                            <div class="panel-heading">
                                <div class="row">
                                    <div class="col-xs-3">
                                        <img src="~/images/monitor.jpg" width="70" />
                                    </div>
                                    <div class="col-xs-9 text-right">
                                        <div class="huge">@ViewData["MonitorCount"]</div>
                                        <div><a asp-controller="Monitors" asp-action="Create" style="color:white">New Monitor!</a></div>
                                    </div>
                                </div>
                            </div>
                            <a asp-controller="Monitors" asp-action="Index">
                                <div class="panel-footer">
                                    <span class="pull-left">View Details</span>
                                    <span class="pull-right"><i class="fa fa-arrow-circle-right"></i></span>
                                    <div class="clearfix"></div>
                                </div>
                            </a>
                        </div>
                    </div>
                    <div class="col-lg-3 col-md-6">
                        <div class="panel panel-yellow">
                            <div class="panel-heading">
                                <div class="row">
                                    <div class="col-xs-3">

                                        <img src="~/images/router.png" width="70" />
                                    </div>
                                    <div class="col-xs-9 text-right">
                                        <div class="huge">@ViewData["NetworkCount"]</div>
                                        <div><a asp-controller="Networks" asp-action="Create" style="color:white">New Network!</a></div>
                                    </div>
                                </div>
                            </div>
                            <a asp-controller="Networks" asp-action="Index">
                                <div class="panel-footer">
                                    <span class="pull-left">View Details</span>
                                    <span class="pull-right"><i class="fa fa-arrow-circle-right"></i></span>
                                    <div class="clearfix"></div>
                                </div>
                            </a>
                        </div>
                    </div>

                    <div class="col-lg-3 col-md-6">
                        <div class="panel panel-red">
                            <div class="panel-heading">
                                <div class="row">
                                    <div class="col-xs-3">

                                        <img src="~/images/printer.png" width="70" />
                                    </div>
                                    <div class="col-xs-9 text-right">
                                        <div class="huge">@ViewData["PrinterCount"]</div>
                                        <div><a asp-controller="Printers" asp-action="Create" style="color:white">New Printer!</a></div>
                                    </div>
                                </div>
                            </div>
                            <a asp-controller="Printers" asp-action="Index">
                                <div class="panel-footer">
                                    <span class="pull-left">View Details</span>
                                    <span class="pull-right"><i class="fa fa-arrow-circle-right"></i></span>
                                    <div class="clearfix"></div>
                                </div>
                            </a>
                        </div>
                    </div>
                </div>


                <!-- /.row -->
                <div class="row">
                    <div class="col-lg-3 col-md-6">
                        <div class="panel panel-primary">
                            <div class="panel-heading">
                                <div class="row">
                                    <div class="col-xs-3">
                                        <img src="~/images/miscellaneous.png" width="80" />
                                    </div>
                                    <div class="col-xs-9 text-right">
                                        <div class="huge">@ViewData["MiscellaneousCount"]</div>
                                        <div><a asp-controller="Miscellaneous" asp-action="Create" style="color:white">New Miscellaneous!</a></div>

                                    </div>
                                </div>
                            </div>
                            <a asp-controller="Miscellaneous" asp-action="Index">
                                <div class="panel-footer">
                                    <span class="pull-left">View Details</span>
                                    <span class="pull-right"><i class="fa fa-arrow-circle-right"></i></span>
                                    <div class="clearfix"></div>
                                </div>
                            </a>
                        </div>
                    </div>
                    <div class="col-lg-3 col-md-6">
                        <div class="panel panel-green">
                            <div class="panel-heading">
                                <div class="row">
                                    <div class="col-xs-3">
                                        <img src="~/images/four-browser-icons.png" width="120" />
                                    </div>
                                    <div class="col-xs-9 text-right">
                                        <div class="huge">@ViewData["IssuesCount"]</div>
                                        <div><a asp-controller="WebCompatibilityIssues" asp-action="Create" style="color:white">New Compatibility Issue!</a></div>
                                    </div>
                                </div>
                            </div>
                            <a asp-controller="WebCompatibilityIssues" asp-action="Index">
                                <div class="panel-footer">
                                    <span class="pull-left">View Details</span>
                                    <span class="pull-right"><i class="fa fa-arrow-circle-right"></i></span>
                                    <div class="clearfix"></div>
                                </div>
                            </a>
                        </div>
                    </div>
                </div>

            </div>
        </div>
    </div>
</div>

